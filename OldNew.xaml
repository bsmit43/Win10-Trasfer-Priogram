<Page x:Class="Windows10FileplusSettingsTransfer.OldNew"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows10FileplusSettingsTransfer"
        mc:Ignorable="d"
        Title="Windows 10 File and Settings Transfer" Height="600" Width="800">
    <Grid>
        <Button Background="Transparent" Content="Old Computer"
		HorizontalAlignment="Left"
		VerticalAlignment="Top" Width="751" 
        Margin="21,43,0,0" Height="181"
		Click="Button_Click"  FontSize="26.667" FontFamily="Microsoft Sans Serif" FontWeight="Bold"/>
        <Button Background="Transparent" Content="New Computer"
        	HorizontalAlignment="Left"
        	VerticalAlignment="Top" Width="751" 
        Margin="21,258,0,0" Height="181"
        	Click="Button_Click_1" FontSize="26.667" FontFamily="Microsoft Sans Serif" FontWeight="Bold"/>
        <DockPanel>
            <Frame x:Name="_oldNewFrame" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Page>
